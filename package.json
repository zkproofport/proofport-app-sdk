{
  "name": "@zkproofport-app/sdk",
  "version": "0.1.0-beta.1",
  "description": "ZKProofport SDK for requesting zero-knowledge proofs from the ZKProofport mobile app and verifying them on-chain",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest",
    "demo": "cd demo && node server.js",
    "docs": "typedoc",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "qrcode": "^1.5.3",
    "socket.io-client": "^4.8.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/qrcode": "^1.5.5",
    "rollup": "^4.9.6",
    "tslib": "^2.8.1",
    "typedoc": "^0.28.16",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "ethers": "^5.7.0 || ^6.0.0"
  },
  "peerDependenciesMeta": {
    "ethers": {
      "optional": true
    }
  },
  "keywords": [
    "zk",
    "zero-knowledge",
    "proof",
    "verification",
    "ethereum",
    "blockchain",
    "zkproofport"
  ],
  "author": "ZKProofport",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/zkproofport/proofport-app-sdk.git"
  },
  "homepage": "https://github.com/zkproofport/proofport-app-sdk#readme",
  "bugs": {
    "url": "https://github.com/zkproofport/proofport-app-sdk/issues"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "sideEffects": false
}
